ylen-transvaginal-1989,\n  author = {Haylen, B. T. and Frazer, M. I. and Sutherst, J. R. and West, C. R.},\n  title = {Transvaginal Ultrasound in the Assessment of Bladder Volumes in Women:   Preliminary Report},\n  journal = {British Journal of Urology},\n  volume = {63},\n  number = {2},\n  pages = {149--151},\n  year = {1989},\n}\n\n\n@Article{hoadley_bayesian_1970,\n  author = {Hoadley, Bruce},\n  title = {A Bayesian Look at Inverse Linear Regression},\n  journal = {Journal of the American Statistical Association},\n\tvolume = {65},\n\tnumber = {329},\n\tyear = {1970},\n\tpages = {356--369},\n}\n\n\n@Article{efron-better-1987,\n  journal = {Journal of the American Statistical Association},\n  author  = {Efron, B.},\n  number  = {397},\n  pages   = {171--185},\n  title   = {Better Bootstrap Confidence Intervals},\n  volume  = {82},\n  year    = {1987},\n}\n\n\n@Article{hall-bootstrap-1986,\n  journal = {The Annals of Statistics},\n  author  = {Hall, Peter},\n  number  = {4},\n  pages   = {1431--1452},\n  title   = {On the Bootstrap and Confidence Intervals},\n  volume  = {14},\n  year    = {1986},\n}\n\n\n@Article {bellio-likelihood-2003,\n  author  = {Bellio, Ruggero},\n  title   = {Likelihood Methods for Controlled Calibration},\n  journal = {Scandinavian Journal of Statistics},\n  volume  = {30},\n  number  = {2},\n  pages   = {339--353},\n  year    = {2003},\n}\n\n@Article{hoadley-bayesian-1970,\n  author  = {Hoadley, Bruce},\n  title   = {A Bayesian Look at Inverse Linear Regression},\n  journal = {Journal of the American Statistical Association},\n  volume  = {65},\n  number  = {329},\n  pages   = {356-369},\n  year    = {1970},\n}\n\n@Article{laird-random-1982,\n  author  = {Nan M. Laird and James H. Ware},\n  title   = {Random-Effects Models for Longitudinal Data},\n  journal = {Biometrics},\n  volume  = {38},\n  number  = {4},\n  pages   = {963--974},\n  year    = {1982},\n}\n\n@Article{osborne-statistical-1991,\n  author  = {Osborne, C.},\n  title   = {